{
  "name": "microservice-sample",
  "version": "0.1.0",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon --watch src --exec node src/index.js",
    "seed": "node scripts/seed-local.js",
    "start:local": "npm run seed && npm start",
    "migrate": "node scripts/migrate.js",
    "migrate:qris": "node scripts/migrate-qris.js",
    "deploy:fly": "flyctl deploy --config fly.toml",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "test": "jest --runInBand --coverage --roots src tests",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "express": "^4.18.2",
    "dotenv": "^16.0.3",
    "morgan": "^1.10.0",
    "winston": "^3.9.0",
    "prom-client": "^14.0.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.0",
    "helmet": "^7.0.0",
    "cors": "^2.8.5",
    "uuid": "^9.0.0",
    "better-sqlite3": "^8.4.0",
    "express-validator": "^7.0.1",
    "express-rate-limit": "^7.0.0",
    "lowdb": "^6.0.1",
    "stripe": "^12.14.0",
    "@sendgrid/mail": "^8.1.6",
    "axios": "^1.5.0"
  },
  "overrides": {
    "axios": ">=0.30.2",
    "semver": ">=7.5.2",
    "simple-update-notifier": ">=1.1.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.11",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^2.8.8",
    "jest": "^29.6.0",
    "supertest": "^6.3.3"
  }
}
